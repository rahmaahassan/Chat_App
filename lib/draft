                              TextField(
                                controller: _controller,
                                decoration:
                                    InputDecoration(labelText: 'Enter your message...'),
                              ),
                              RaisedButton(
                                color: Colors.red.shade200,
                                child: Text('Add Message'),
                                onPressed: () {
                                  if (_controller.text.isNotEmpty) {
                                    setState(() {
                                      msg.add(_controller.text);
                                      _controller.clear();
                                      saveDta();
                                    });
                                  }
                                },
